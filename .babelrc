{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "last 2 versions"
          ]
        }
      }
    ],
    [
      "@babel/preset-stage-1",
      {
        "decoratorsLegacy": true
      }
    ]
  ],
  "plugins": [
    "transform-decorators-legacy"
  ],
  "ignore": [
    "node_modules",
    "build"
  ]
}

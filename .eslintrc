{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
  },
  "extends": [
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "plugin:react/recommended",
    "plugin:css-modules/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "react",
    "jsx-a11y",
    "filenames",
    "prettier",
    "css-modules"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".jsx",
          ".js"
        ]
      }
    ],
    "react/prefer-stateless-function": 0,
    "prettier/prettier": 2,
    "linebreak-style": 0,
    "global-require": 0,
    "eol-last": 1,
    "comma-dangle": 0,
    "spaced-comment": 0,
    "arrow-body-style": 1,
    "camelcase": 1
  }
}
